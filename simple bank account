class BankAccount {
    balance: number = 0;

    deposit(amount: number) {
        this.balance += amount;
    }

    withdraw(amount: number) {
        if (amount <= this.balance) this.balance -= amount;
    }
}

let acc = new BankAccount();
acc.deposit(100);
acc.withdraw(30);
console.log(acc.balance); // 70
